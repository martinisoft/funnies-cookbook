server {
  listen 80;
  server_name funniesapp.com www.funniesapp.com;
  root /srv/funnies/current/public;
  access_log /var/log/nginx/funniesapp-access.log;

  passenger_enabled on;

  location ~ ^/assets/ {
    expires 1y;
    add_header Cache-Control public;

    add_header ETag "";
    break;
  }
}
